# Example environment variables for akahu-mortgage-dashboard
# Copy this to .env and edit values before running locally or in Docker.

# Path to the DuckDB database file. In Docker this is usually mounted at /data/akahu.duckdb
DUCKDB_PATH=/data/akahu.duckdb

# Flask dev settings (optional)
FLASK_ENV=development
FLASK_DEBUG=1

# Akahu API tokens
AKAHU_USER_TOKEN=YOUR_AKAHU_USER_TOKEN_HERE
AKAHU_APP_TOKEN=YOUR_AKAHU_APP_TOKEN_HERE
